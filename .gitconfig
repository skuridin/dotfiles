[color]
	ui = auto
[push]
	default = current
[alias]
	cho = checkout
	new = checkout -B
	n = checkout -B
	delete = branch -D
	d = branch -D
	rename = branch -m
	am = commit --amend
	pr = pull --rebase
	fpush = push -u --force-with-lease origin HEAD
	cp = cherry-pick
	l = log --oneline --graph --all
[user]
	name = Evgeniy Skuridin
	email = i@skurid.in
[core]
	#excludesfile = ~/.gitignore_global
[diff "sopsdiffer"]
	textconv = sops -d
[credential]
	helper = store
[init]
	defaultBranch = main
